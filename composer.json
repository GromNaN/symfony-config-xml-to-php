{
    "name": "gromnan/symfony-config-xml-to-php",
    "type": "library",
    "description": "Convert Symfony config files from XML to PHP",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "GromNaN\\SymfonyConfigXmlToPhp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/convert"
    ],
    "authors": [
        {
            "name": "Jérôme Tamarelle",
            "email": "jerome@tamarelle.net"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">= 8.2",
        "ext-dom": "*",
        "symfony/finder": "7.4.*",
        "symfony/config": "7.4.*",
        "symfony/console": "7.4.*",
        "symfony/dependency-injection": "7.4.*",
        "symfony/expression-language": "7.4.*",
        "sebastian/diff": "^6.0 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5 || ^12.5 || ^13.0@dev"
    },
    "scripts": {
        "test": "phpunit"
    }
}
